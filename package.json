{
  "name": "api-with-docker-composer",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.12",
    "npm-run-all": "^4.1.5",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "pm2": "^4.5.1",
    "express": "^4.17.1"
  },
  "scripts": {
    "build": "yarn tsc",
    "start": "npm-run-all start:*",
    "start:build": "yarn tsc",
    "start:pm2": "pm2 start ./dist/Server.js --name dev-server --watch --no-daemon",
    "dev": "npm-run-all -p dev:*",
    "dev:server": "ts-node-dev --trace-deprecation -r tsconfig-paths/register --transpile-only --ignore-watch node_modules ./src/Server.ts"
  }
}
